## Catalog Record ##

ex:CatalogRecord a sh:NodeShape, rdfs:Class ;
 sh:targetClass  dcat:CatalogRecord ;
  # sh:property [
  #   sh:group        ex:GroepCR ;
  #   sh:datatype     rdf:langString ;
  #   sh:languageIn   ( "nl" "en" ) ;
  #   sh:uniqueLang   true ;
  #   sh:name         "title" ;
  #   sh:path         dct:title
  # ] ;

  sh:property [
    sh:group        ex:GroepCR ;
    sh:nodeKind     sh:IRI ;
    sh:minCount     1 ;
    sh:maxCount     1 ;
    sh:name         "primary topic" ;
    sh:defaultValue "http://example.org/dataset/42" ;
    sh:path         foaf:primaryTopic ;
  ] ;

  sh:property [
    sh:group        ex:GroepCR ;
    sh:datatype     xsd:date ;
    sh:minCount     1 ;
    sh:maxCount     1 ;
    sh:name         "modification date" ;
    sh:path         dct:modified ;
  ] ;
 
  sh:property [
    sh:group        ex:GroepCR ;
    # sh:nodeKind     dct:LinguisticSystem ;
    sh:nodeKind     sh:IRI ;
    sh:minCount     1 ;
    sh:name         "language" ;
    sh:path         dct:language ;
    sh:in ( <http://publications.europa.eu/resource/authority/language/NLD> )
  ] ;

  sh:property [
    sh:group        ex:GroepCR ;
    # sh:nodeKind     dct:Standard ;
    sh:nodeKind     sh:IRI ;
    sh:name         "application profile" ;
    sh:path         dct:conformsTo ;
    sh:in ( <https://semiceu.github.io/DCAT-AP/releases/3.0.0> )
  ] ;
  
.
